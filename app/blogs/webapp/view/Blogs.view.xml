<mvc:View
    xmlns:smartform="sap.ui.comp.smartform"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:semantic="sap.f.semantic"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns="sap.uxap"
    xmlns:m="sap.m"
    controllerName="blogs.controller.Blogs"
    displayBlock="true"
>
    <semantic:SemanticPage id="_IDGenSemanticPage1">
        <semantic:titleHeading>
            <m:Title
                id="_IDGenTitle6"
                text="Personal Information"
            />
        </semantic:titleHeading>
        <semantic:headerContent>
            <layout:ResponsiveFlowLayout id="_IDGenResponsiveFlowLayout1">
                <layout:VerticalLayout id="_IDGenVerticalLayout1">
                    <m:Title
                        id="_IDGenTitle4"
                        text="General"
                    />
                    <m:HBox
                        id="_IDGenHBox7"
                        class="sapUiTinyMarginBottom"
                    >
                        <m:Text
                            id="_IDGenText1"
                            text="Name"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox id="_IDGenHBox8">
                        <m:Text
                            id="_IDGenText2"
                            text="test"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                </layout:VerticalLayout>
                <layout:VerticalLayout id="_IDGenVerticalLayout2">
                    <m:Title
                        id="_IDGenTitle5"
                        text="Interactions"
                    />
                    <m:HBox
                        id="_IDGenHBox4"
                        class="sapUiTinyMarginBottom"
                    >
                        <core:Icon
                            id="_IDGenIcon1"
                            src="sap-icon://thumb-up"
                        />
                        <m:Label
                            id="_IDGenLabel3"
                            text="Likes"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox
                        id="_IDGenHBox5"
                        class="sapUiTinyMarginBottom"
                    >
                        <core:Icon
                            id="_IDGenIcon2"
                            src="sap-icon://thumb-down"
                        />
                        <m:Label
                            id="_IDGenLink2"
                            text="Dislikes"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox id="_IDGenHBox6">
                        <core:Icon
                            id="_IDGenIcon3"
                            src="sap-icon://favorite-list"
                        />
                        <m:Label
                            id="_IDGenLink3"
                            text="Reputation"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                </layout:VerticalLayout>
            </layout:ResponsiveFlowLayout>
        </semantic:headerContent>

        <semantic:content>
            <m:Table
                id="idProductsTable"
                items="{/Blogs}"
            >
                <m:headerToolbar>
                    <m:OverflowToolbar>
                        <m:Title
                            text="Blogs"
                            level="H2"
                        />
                        <m:ToolbarSpacer />
                        <m:Button
                            icon="sap-icon://add"
                            press="onOpenDialog"
                        />
                    </m:OverflowToolbar>
                </m:headerToolbar>
                <m:columns>
                    <m:Column id="_IDGenColumn1">
                        <m:Text
                            id="_IDGenText3"
                            text="ID"
                        />
                    </m:Column>
                    <m:Column id="_IDGenColumn2">
                        <m:Text
                            id="_IDGenText4"
                            text="Title"
                        />
                    </m:Column>
                    <m:Column id="_IDGenColumn3">
                        <m:Text
                            id="_IDGenText5"
                            text="Likes"
                        />
                    </m:Column>
                    <m:Column id="_IDGenColumn6">
                        <m:Text
                            id="_IDGenText13"
                            text="Dislikes"
                        />
                    </m:Column>
                    <m:Column id="_IDGenColumn4">
                        <m:Text
                            id="_IDGenText6"
                            text="Favorites"
                        />
                    </m:Column>
                    <m:Column id="_IDGenColumn5">
                        <m:Text
                            id="_IDGenText7"
                            text="Created By"
                        />
                    </m:Column>
                </m:columns>
                <m:items>
                    <m:ColumnListItem
                        id="_IDGenColumnListItem1"
                        type="Navigation"
                        press=".onListItemPressed"
                    >
                        <m:cells>
                            <m:ObjectIdentifier
                                id="_IDGenObjectIdentifier1"
                                title="{ID}"
                                text="{content}"
                            />
                            <m:Text
                                id="_IDGenText8"
                                text="{title}"
                            />
                            <m:Text
                                id="_IDGenText9"
                                text="{likes}"
                            />
                            <m:Text
                                id="_IDGenText12"
                                text="{dislikes}"
                            />
                            <m:Text
                                id="_IDGenText10"
                                text="{favorites}"
                            />
                            <m:Text
                                id="_IDGenText11"
                                text="{createdBy}"
                            />
                        </m:cells>
                    </m:ColumnListItem>
                </m:items>
            </m:Table>
        </semantic:content>
    </semantic:SemanticPage>
</mvc:View>
