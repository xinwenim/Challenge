<mvc:View
    xmlns:smartform="sap.ui.comp.smartform"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:semantic="sap.f.semantic"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns="sap.uxap"
    xmlns:m="sap.m"
    controllerName="blogs.controller.Blogs"
    displayBlock="true"
>
    <semantic:SemanticPage id="_IDGenSemanticPage1">
        <semantic:titleHeading>
            <m:Title
                id="_IDGenTitle6"
                text="Personal Information"
            />
        </semantic:titleHeading>
        <semantic:headerContent>
            <layout:ResponsiveFlowLayout id="_IDGenResponsiveFlowLayout1">
                <layout:VerticalLayout id="_IDGenVerticalLayout1">
                    <m:Title
                        id="_IDGenTitle4"
                        text="General"
                    />
                    <m:HBox
                        id="_IDGenHBox7"
                        class="sapUiTinyMarginBottom"
                    >
                        <m:Text
                            id="_IDGenText1"
                            text="Name"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox id="_IDGenHBox8">
                        <m:Text
                            id="_IDGenText2"
                            text="test"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                </layout:VerticalLayout>

                <layout:VerticalLayout id="_IDGenVerticalLayout2">
                    <m:Title
                        id="_IDGenTitle5"
                        text="Interactions"
                    />

                    <m:HBox
                        id="_IDGenHBox4"
                        class="sapUiTinyMarginBottom"
                    >
                        <core:Icon
                            id="_IDGenIcon1"
                            src="sap-icon://thumb-up"
                        />
                        <m:Label
                            id="_IDGenLabel3"
                            text="Likes"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox
                        id="_IDGenHBox5"
                        class="sapUiTinyMarginBottom"
                    >
                        <core:Icon
                            id="_IDGenIcon2"
                            src="sap-icon://thumb-down"
                        />
                        <m:Label
                            id="_IDGenLink2"
                            text="Dislikes"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                    <m:HBox id="_IDGenHBox6">
                        <core:Icon
                            id="_IDGenIcon3"
                            src="sap-icon://favorite-list"
                        />
                        <m:Label
                            id="_IDGenLink3"
                            text="Reputation"
                            class="sapUiSmallMarginBegin"
                        />
                    </m:HBox>
                </layout:VerticalLayout>
            </layout:ResponsiveFlowLayout>
        </semantic:headerContent>
        <semantic:content>
            <m:Table
                id="idBlogsTable"
                inset="false"
                items="{path: '/Blogs'}"
            >
                <m:headerToolbar>
                    <m:Toolbar id="_IDGenToolbar1">
                        <m:content>
                            <m:Title
                                id="title"
                                text="Blogs"
                            />
                            <m:ToolbarSpacer id="_IDGenToolbarSpacer1" />
                            <m:Button id="_IDGenButton1"
                                icon="sap-icon://action-settings"
                                press="openPersoDialog"
                            />
                        </m:content>
                    </m:Toolbar>
                </m:headerToolbar>
                <m:columns>
                    <m:Column
                        id="_IDGenColumn1"
                        width="12em"
                    >
                        <m:Text
                            id="_IDGenText3"
                            text="ID"
                        />
                    </m:Column>
                    <m:Column
                        id="_IDGenColumn2"
                        minScreenWidth="Tablet"
                        demandPopin="true"
                    >
                        <m:Text
                            id="_IDGenText4"
                            text="Title"
                        />
                    </m:Column>
                </m:columns>
                <m:items>
                    <m:ColumnListItem
                        id="_IDGenColumnListItem1"
                        vAlign="Middle"
                        type="Navigation"
                        press=".onItemPress"
                    >
                        <m:cells>
                            <m:Text
                                id="_IDGenObjectIdentifier1"
                                text="{ID}"
                            />
                            <m:Text
                                id="_IDGenText5"
                                text="{title}"
                            />
                        </m:cells>
                    </m:ColumnListItem>
                </m:items>
            </m:Table>
        </semantic:content>
    </semantic:SemanticPage>
</mvc:View>
