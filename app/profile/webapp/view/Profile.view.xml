<mvc:View controllerName="profile.controller.Profile"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.uxap"
	xmlns:layout="sap.ui.layout"
	xmlns:forms="sap.ui.layout.form"
	xmlns:m="sap.m"
	>
<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="true" upperCaseAnchorBar="false">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle id="_IDGenObjectPageDynamicHeaderTitle1">
				<actions>
					<m:Button id="_IDGenButton2" text="Retake MBTI Test" press="onRetakeTest"/>
				</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>

		<headerContent>
			<m:FlexBox id="_IDGenFlexBox2" wrap="Wrap" fitContainer="true" >
				<m:Avatar id="idProfilePic" class="sapUiSmallMarginEnd" displaySize="L" />

				<layout:VerticalLayout id="_IDGenVerticalLayout3" class="sapUiSmallMarginBeginEnd">
				<layout:ResponsiveFlowLayout >
				<m:Title id="idName" text="" wrapping="true"/>
					<m:Label id="labelRegion" text=""/>
					<m:Link id="labelEmail" text=""/>
				</layout:ResponsiveFlowLayout>
				</layout:VerticalLayout>
			</m:FlexBox>
		</headerContent>

		<sections>

			<ObjectPageSection id="_IDGenObjectPageSection11" titleUppercase="false" title="My MBTI">
				<subSections>
					<ObjectPageSubSection id="_IDGenObjectPageSubSection11" titleUppercase="false">
						<forms:Form id="_IDGenForm11" width="100%">
							<forms:layout>
								<forms:ResponsiveGridLayout id="_IDGenColumnLayout11"/>
							</forms:layout>
							<forms:formContainers>
								<forms:FormContainer id="socialInfo" title="">
									<forms:formElements>
										<forms:FormElement label="Type">
											<forms:fields>
												<m:Text id="textType" text=""/>
											</forms:fields>
										</forms:FormElement>
										<forms:FormElement label="Description">
											<forms:fields>
												<m:Text id="textDes" text=""/>
											</forms:fields>
										</forms:FormElement>

									</forms:formElements>
								</forms:FormContainer>

							</forms:formContainers>

						</forms:Form>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="_IDGenObjectPageSection1" titleUppercase="false" title="Status">
				<subSections>
					<ObjectPageSubSection id="_IDGenObjectPageSubSection1" titleUppercase="false">
						<forms:Form id="_IDGenForm1" width="100%">
							<forms:layout>
								<forms:ColumnLayout id="_IDGenColumnLayout1"/>
							</forms:layout>
							<forms:formContainers>

								<forms:FormContainer id="addressInfo" title="">
									<forms:formElements>

										<forms:FormElement id="_IDGenFormElement1" label="Mind">
											<forms:fields>
												<m:Text id="_IDGenText3" text="65"/>
											</forms:fields>
										</forms:FormElement>
										<forms:FormElement id="_IDGenFormElement11">

										</forms:FormElement>

										<forms:FormElement id="_IDGenFormElement2" label="Sport">
											<forms:fields>
												<m:Text id="_IDGenText4" text="35"/>
											</forms:fields>
										</forms:FormElement>
										<forms:FormElement id="_IDGenFormElement12">

										</forms:FormElement>
										<forms:FormElement id="_IDGenFormElement3" label="Food">
											<forms:fields>
												<m:Text id="_IDGenText5" text="50"/>
											</forms:fields>
										</forms:FormElement>
									</forms:formElements>
								</forms:FormContainer>

								<forms:FormContainer id="socialInfo1" title=" ">
									<forms:formElements>

									</forms:formElements>
								</forms:FormContainer>
							</forms:formContainers>
						</forms:Form>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>
