<mvc:View controllerName="profile.controller.Profile"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:m="sap.m" xmlns="sap.suite.ui.microchart">
    <m:GenericTile id="_IDGenGenericTile1" header="Profile"
		frameType="OneByOne" press="onToView2">
		<m:tileContent>
			<m:TileContent id="_IDGenTileContent1"  footer=" Mind /// Sport /// Food">
				<m:content>
					<ColumnMicroChart id="_IDGenColumnMicroChart1" size="Responsive">
						<columns>
							<ColumnMicroChartData id="_IDGenColumnMicroChartData1" value="65" color="Good" />
							<ColumnMicroChartData id="_IDGenColumnMicroChartData2" value="35" color="Error" />
							<ColumnMicroChartData id="_IDGenColumnMicroChartData3" value="50" color="Critical" />
						</columns>
					</ColumnMicroChart>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
</mvc:View>