<mvc:View
    controllerName="recording.controller.Calendar"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:mc="sap.suite.ui.microchart"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    class="viewPadding"
>
    <l:DynamicSideContent
        id="DynamicSideContent"
        class="sapUiDSCExplored"
        sideContentFallDown="BelowM"
        sideContentPosition="End"
        containerQuery="true"
        showSideContent="false"
        breakpointChanged="updateToggleButtonState"
    >
        <l:mainContent>
            <l:VerticalLayout id="_IDGenVerticalLayout1">
                <FlexBox
                    id="_IDGenFlexBox1"
                    direction="Column"
                >
                    <Button
                        id="_IDGenButton1"
                        press="handleSelectToday"
                        text="Select Today"
                    />
                    <l:HorizontalLayout id="_IDGenHorizontalLayout1">
                        <Label
                            id="_IDGenLabel1"
                            text="Selected Date:"
                            class="labelMarginLeft"
                        />
                        <Text
                            id="selectedDate"
                            text=""
                            class="labelMarginLeft"
                        />
                    </l:HorizontalLayout>
                </FlexBox>
                <u:Calendar
                    id="Calendar"
                    width="70rem"
                    select="handleCalendarSelect"
                />
                <Panel
                    id="_IDGenPanel1"
                    headerText="Mind · Health · Hunger"
                    height="10rem"
                    width="70rem"
                >
                    <l:Grid id="_IDGenGrid1">
                        <FlexBox
                            id="_fex"
                            width="25rem"
                            height="9rem"
                            alignItems="Stretch"
                            alignContent="Stretch"

                        >
                            <items>
                                <mc:InteractiveLineChart
                                    id="_IDGenInteractiveLineChart2"
                                >
                                    <mc:points>
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint8"
                                            value="33.1"
                                            label="Mon"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint9"
                                            value="12"
                                            label="Tue"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint10"
                                            value="51.4"
                                            label="Wed"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint11"
                                            value="52"
                                            label="Thu"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint12"
                                            value="69.9"
                                            label="Fri"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint13"
                                            value="0.9"
                                            label="Sat"
                                            color="Error"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint14"
                                            value="0.9"
                                            label="Sun"
                                            color="Error"
                                        />
                                    </mc:points>
                                </mc:InteractiveLineChart>
                                <mc:InteractiveLineChart
                                    id="_IDGenInteractiveLineChart1"
                                >
                                    <mc:points>
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint1"
                                            value="33.1"
                                            label="Mon"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint2"
                                            value="12"
                                            label="Tue"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint3"
                                            value="51.4"
                                            label="Wed"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint4"
                                            value="52"
                                            label="Thu"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint5"
                                            value="69.9"
                                            label="Fri"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint6"
                                            value="0.9"
                                            label="Sat"
                                            color="Good"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint7"
                                            value="0.9"
                                            label="Sun"
                                            color="Good"
                                        />
                                    </mc:points>
                                </mc:InteractiveLineChart>
                                <mc:InteractiveLineChart
                                    id="_IDGenInteractiveLineChart3"
                                >
                                    <mc:points>
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint15"
                                            value="33.1"
                                            label="Mon"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint16"
                                            value="12"
                                            label="Tue"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint17"
                                            value="51.4"
                                            label="Wed"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint18"
                                            value="52"
                                            label="Thu"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint19"
                                            value="69.9"
                                            label="Fri"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint20"
                                            value="0.9"
                                            label="Sat"
                                            color="Critical"
                                        />
                                        <mc:InteractiveLineChartPoint
                                            id="_IDGenInteractiveLineChartPoint21"
                                            value="0.9"
                                            label="Sun"
                                            color="Critical"
                                        />
                                    </mc:points>
                                </mc:InteractiveLineChart>
                            </items>
                        </FlexBox>
                    </l:Grid>
                </Panel>
            </l:VerticalLayout>
        </l:mainContent>
        <l:sideContent>
            <f:DynamicPage
                toggleHeaderOnTitleClick="false"
                showFooter="true"
                id="SidePanel"
                busyIndicatorDelay="0"
            >
                <f:title>
                    <f:DynamicPageTitle id="_IDGenDynamicPageTitle1">
                        <f:heading>
                            <Title
                                id="_IDGenTitle1"
                                text="mesaures"
                                wrapping="false"
                                class="sapUiTinyMarginEnd"
                            />
                        </f:heading>
                        <f:navigationActions>
                            <OverflowToolbarButton
                                id="_IDGenOverflowToolbarButton1"
                                type="Transparent"
                                icon="sap-icon://decline"
                                press="handleClose"
                                tooltip="Close column"
                            />
                        </f:navigationActions>
                    </f:DynamicPageTitle>
                </f:title>
                <f:content>
                    <VBox id="_IDGenVBox2">
                        <FlexBox
                            id="_IDGenFlexBox2"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox1"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon1"
                                    src="sap-icon://heart"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle2"
                                    text="{i18n>general.text.Mind}"
                                />
                                <core:Icon
                                    id="_IDGenIcon4"
                                    src="sap-icon://message-information"
                                    class="assignedBusinessScenario"
                                    press="onPlaceholderPressed"
                                />
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox3"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel2"
                                text="{i18n>general.text.Pressure}:"
                                tooltip="{i18n>general.text.Pressure}:"
                            />
                            <Input id="pressVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox4"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel3"
                                text="{i18n>general.text.SleepTime}:"
                                tooltip="{i18n>general.text.SleepTime}:"
                            />
                            <Input id="sleepVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox5"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel4"
                                text="{i18n>general.text.Mood}:"
                                tooltip="{i18n>general.text.Mood}:"
                            />
                            <Input id="moodVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox6"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox2"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon2"
                                    src="sap-icon://pool"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle3"
                                    text="{i18n>general.text.Health}"
                                />
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox7"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel5"
                                text="{i18n>general.text.MotionTime}:"
                                tooltip="{i18n>general.text.MotionTime}:"
                            />
                            <Input id="motionVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox8"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel6"
                                text="{i18n>general.text.Height}:"
                                tooltip="{i18n>general.text.Height}:"
                            />
                            <Input id="heightVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox9"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel7"
                                text="{i18n>general.text.Weight}:"
                                tooltip="{i18n>general.text.Weight}:"
                            />
                            <Input id="weightVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox10"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox3"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon3"
                                    src="sap-icon://meal"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle4"
                                    text="{i18n>general.text.Hunger}"
                                />
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox11"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel8"
                                text="{i18n>general.text.JunkFood}:"
                                tooltip="{i18n>general.text.JunkFood}:"
                            />
                            <Input id="junkFoodVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox12"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel9"
                                text="{i18n>general.text.Fruit}:"
                                tooltip="{i18n>general.text.Fruit}:"
                            />
                            <Input id="fruitVal" />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox13"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel10"
                                text="{i18n>general.text.Water}:"
                                tooltip="{i18n>general.text.Water}:"
                            />
                            <Input id="waterVal" />
                        </FlexBox>
                    </VBox>
                </f:content>
                <f:footer>
                    <OverflowToolbar id="idOverFlowToolBar">
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            text="{i18n>general.action.submit}"
                            type="Emphasized"
                            id="submitBtn"
                            press="onSubmitButtonPress"
                        />
                        <Button
                            text="{i18n>general.action.close}"
                            id="closeBtn"
                            press="handleClose"
                        />
                    </OverflowToolbar>
                </f:footer>
            </f:DynamicPage>
        </l:sideContent>
    </l:DynamicSideContent>
</mvc:View>
