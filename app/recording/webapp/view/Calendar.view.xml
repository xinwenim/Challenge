<mvc:View
    controllerName="recording.controller.Calendar"
    xmlns:l="sap.ui.layout"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:mc="sap.suite.ui.microchart"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    class="viewPadding"
>
    <l:DynamicSideContent
        id="DynamicSideContent"
        class="sapUiDSCExplored"
        sideContentFallDown="BelowM"
        sideContentPosition="End"
        containerQuery="true"
        showSideContent="false"
        breakpointChanged="updateToggleButtonState"
    >
        <l:mainContent>
            <l:VerticalLayout id="_IDGenVerticalLayout1">
                <FlexBox
                    id="_IDGenFlexBox1"
                    direction="Column"
                >
                    <Button
                        id="_IDGenButton1"
                        press="handleSelectToday"
                        text="Select Today"
                    />
                    <l:HorizontalLayout id="_IDGenHorizontalLayout1">
                        <Label
                            id="_IDGenLabel1"
                            text="Selected Date:"
                            class="labelMarginLeft"
                        />
                        <Text
                            id="selectedDate"
                            text=""
                            class="labelMarginLeft"
                        />
                    </l:HorizontalLayout>
                </FlexBox>
                <u:Calendar
                    id="Calendar"
                    width="70rem"
                    select="handleCalendarSelect"
                />
                <Panel
                    id="_IDGenPanel1"
                    headerText="Measures:  Mind · Health · Hunger"
                    height="10rem"
                    width="70rem"
                >
                    <l:Grid
                        id="_IDGenResponsiveFlowLayout1"
                        defaultSpan="L6 M1 S1"
                        content="{/multiline}"
                    >
                        <mc:LineMicroChart
                            id="_IDGenLineMicroChart1"
                            size="L"
                            color="{color}"
                            showPoints="true"
                            width="60rem"
                            leftBottomLabel="{leftBottomLabel}"
                            rightBottomLabel="{rightBottomLabel}"
                            showTopLabels="true"
                            showBottomLabels="true"

                            lines="{
							path: 'lines',
							templateShareable: true
						}"
                        >
                            <mc:lines>
                                <mc:LineMicroChartLine
                                    id="_IDGenLineMicroChartLine1"
                                    points="{
								path: 'points',
								templateShareable: true
							}"
                                    color="{color}"
                                    showPoints="{showPoints}"
                                    type="{type}"
                                >
                                    <mc:LineMicroChartPoint
                                        id="_IDGenLineMicroChartPoint1"
                                        x="{x}"
                                        y="{y}"
                                    />
                                </mc:LineMicroChartLine>
                            </mc:lines>
                        </mc:LineMicroChart>
                    </l:Grid>
                </Panel>
            </l:VerticalLayout>
        </l:mainContent>
        <l:sideContent>
            <f:DynamicPage
                toggleHeaderOnTitleClick="false"
                showFooter="true"
                id="SidePanel"
                busyIndicatorDelay="0"
            >
                <f:title>
                    <f:DynamicPageTitle id="_IDGenDynamicPageTitle1">
                        <f:heading>
                            <Title
                                id="_IDGenTitle1"
                                text="mesaures"
                                wrapping="false"
                                class="sapUiTinyMarginEnd"
                            />
                        </f:heading>
                        <f:navigationActions>
                            <OverflowToolbarButton
                                id="_IDGenOverflowToolbarButton1"
                                type="Transparent"
                                icon="sap-icon://decline"
                                press="handleClose"
                                tooltip="Close column"
                            />
                        </f:navigationActions>
                    </f:DynamicPageTitle>
                </f:title>
                <f:content>
                    <VBox id="_IDGenVBox2">
                        <FlexBox
                            id="_IDGenFlexBox2"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox1"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon1"
                                    src="sap-icon://heart"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle2"
                                    text="{i18n>general.text.Mind}"
                                />
                             <core:Icon id="_IDGenIcon4"
                             src="sap-icon://message-information"
                              class="assignedBusinessScenario"
                              press="onPlaceholderPressed"/>
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox3"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel2"
                                text="{i18n>general.text.Pressure}:"
                                tooltip="{i18n>general.text.Pressure}:"
                            />
                            <Text
                                id="_IDGenText1"
                                text="50%"
                                tooltip="50%"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox4"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel3"
                                text="{i18n>general.text.SleepTime}:"
                                tooltip="{i18n>general.text.SleepTime}:"
                            />
                            <Text
                                id="_IDGenText2"
                                text="8h"
                                tooltip="8h"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox5"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel4"
                                text="{i18n>general.text.Mood}:"
                                tooltip="{i18n>general.text.Mood}:"
                            />
                            <Text
                                id="_IDGenText3"
                                text="5(Excellent)"
                                tooltip="5(Excellent)"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox6"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox2"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon2"
                                    src="sap-icon://pool"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle3"
                                    text="{i18n>general.text.Health}"
                                />
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox7"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel5"
                                text="{i18n>general.text.MotionTime}:"
                                tooltip="{i18n>general.text.MotionTime}:"
                            />
                            <Text
                                id="_IDGenText4"
                                text="120min"
                                tooltip="120min"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox8"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel6"
                                text="{i18n>general.text.Height}:"
                                tooltip="{i18n>general.text.Height}:"
                            />
                            <Text
                                id="_IDGenText5"
                                text="1.7m"
                                tooltip="1.7m"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox9"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel7"
                                text="{i18n>general.text.Weight}:"
                                tooltip="{i18n>general.text.Weight}:"
                            />
                            <Text
                                id="_IDGenText6"
                                text="75kg"
                                tooltip="75kg"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox10"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <HBox
                                id="_IDGenHBox3"
                                alignItems="Center"
                            >
                                <core:Icon
                                    id="_IDGenIcon3"
                                    src="sap-icon://meal"
                                    color="Neutral"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Title
                                    id="_IDGenTitle4"
                                    text="{i18n>general.text.Hunger}"
                                />
                            </HBox>
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox11"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel8"
                                text="{i18n>general.text.JunkFood}:"
                                tooltip="{i18n>general.text.JunkFood}:"
                            />
                            <Text
                                id="_IDGenText7"
                                text="0 times"
                                tooltip="0 times"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox12"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel9"
                                text="{i18n>general.text.Fruit}:"
                                tooltip="{i18n>general.text.Fruit}:"
                            />
                            <Text
                                id="_IDGenText8"
                                text="2 times"
                                tooltip="2 times"
                            />
                        </FlexBox>
                        <FlexBox
                            id="_IDGenFlexBox13"
                            direction="Column"
                            class="sapUiTinyMarginBottom"
                        >
                            <Label
                                id="_IDGenLabel10"
                                text="{i18n>general.text.Water}:"
                                tooltip="{i18n>general.text.Water}:"
                            />
                            <Text
                                id="_IDGenText9"
                                text="3 times"
                                tooltip="3 times"
                            />
                        </FlexBox>
                    </VBox>
                </f:content>
                <f:footer>
                    <OverflowToolbar id="_IDGenOverflowToolbar1">
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            text="{i18n>general.action.submit}"
                            type="Emphasized"
                            id="submitBtn"
                            press="onSubmitButtonPress"
                        />
                        <Button
                            text="{i18n>general.action.close}"
                            id="closeBtn"
                            press="handleClose"
                        />
                    </OverflowToolbar>
                </f:footer>
            </f:DynamicPage>
        </l:sideContent>
    </l:DynamicSideContent>
</mvc:View>
